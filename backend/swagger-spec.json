  "info": {
    "title": "My API",
    "description": "The backend API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "supabase-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "description": "Enter your Supabase access token"
      }
    },
    "schemas": {
      "CommonResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "email_verified": {
            "type": "boolean"
          },
          "username": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "lastSignInAt": {
            "type": "string"
          },
          "provider": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "email",
          "email_verified",
          "username",
          "role",
          "lastSignInAt",
          "provider"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserResponseDto"
          }
        },
        "required": [
          "accessToken",
          "user"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "johndoe@example.com",
            "description": "User email address"
          },
          "password": {
            "type": "string",
            "example": "secret123",
            "description": "Password for the user"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "yizunwong@gmail.com",
            "description": "User email address"
          },
          "username": {
            "type": "string",
            "example": "johndoe",
            "description": "Unique username for the user"
          },
          "password": {
            "type": "string",
            "example": "StrongPass123!",
            "description": "Password (at least 6 characters)"
          },
          "confirmPassword": {
            "type": "string",
            "example": "StrongPass123!",
            "description": "Confirm password (at least 6 characters)"
          }
        },
        "required": [
          "email",
          "username",
          "password",
          "confirmPassword"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "johndoe@example.com",
            "description": "User email address"
          },
          "username": {
            "type": "string",
            "example": "johndoe",
            "description": "Username for the user"
          }
        },
        "required": [
          "email",
          "username"
        ]
      },
      "UploadClaimDocDto": {
        "type": "object",
        "properties": {
          "claim_id": {
            "type": "number",
            "example": 1,
            "description": "ID of the claim to which the document belongs"
          },
          "name": {
            "type": "string",
            "example": "accident_report.pdf",
            "description": "Name of the uploaded document"
          },
          "url": {
            "type": "string",
            "example": "https://example.com/uploads/accident_report.pdf",
            "description": "URL of the uploaded document"
          }
        },
        "required": [
          "claim_id",
          "name",
          "url"
        ]
      },
      "CreateClaimDto": {
        "type": "object",
        "properties": {
          "policy_id": {
            "type": "number",
            "example": 1,
            "description": "ID of the policy associated with the claim"
          },
          "claim_type": {
            "type": "string",
            "example": "accident",
            "description": "Type of the claim"
          },
          "amount": {
            "type": "number",
            "example": 1000,
            "description": "Amount claimed"
          },
          "status": {
            "type": "string",
            "example": "pending",
            "description": "Status of the claim"
          },
          "description": {
            "type": "string",
            "example": "Accident on highway, car damaged.",
            "description": "Description of the claim"
          },
          "documents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UploadClaimDocDto"
            }
          }
        },
        "required": [
          "policy_id",
          "claim_type",
          "amount",
          "status",
          "documents"
        ]
      },
      "UpdateClaimDto": {
        "type": "object",
        "properties": {
          "policy_id": {
            "type": "number",
            "example": 1,
            "description": "ID of the policy associated with the claim"
          },
          "claim_type": {
            "type": "string",
            "example": "accident",
            "description": "Type of the claim"
          },
          "amount": {
            "type": "number",
            "example": 1000,
            "description": "Amount claimed"
          },
          "status": {
            "type": "string",
            "example": "pending",
            "description": "Status of the claim"
          },
          "description": {
            "type": "string",
            "example": "Accident on highway, car damaged.",
            "description": "Description of the claim"
          },
          "documents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UploadClaimDocDto"
            }
          },
          "id": {
            "type": "number",
            "example": 1,
            "description": "ID of the claim to update"
          }
        },
        "required": [
          "id"
        ]
      }
    }
  }
}