  "info": {
    "title": "My API",
    "description": "The backend API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "supabase-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "description": "Enter your Supabase access token"
      }
    },
    "schemas": {
      "CommonResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "email_verified": {
            "type": "boolean"
          },
          "username": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "lastSignInAt": {
            "type": "string"
          },
          "provider": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "email",
          "email_verified",
          "username",
          "role",
          "lastSignInAt",
          "provider"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserResponseDto"
          }
        },
        "required": [
          "accessToken",
          "user"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "johndoe@example.com",
            "description": "User email address"
          },
          "password": {
            "type": "string",
            "example": "secret123",
            "description": "Password for the user"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "yizunwong@gmail.com",
            "description": "User email address"
          },
          "username": {
            "type": "string",
            "example": "johndoe",
            "description": "Unique username for the user"
          },
          "password": {
            "type": "string",
            "example": "StrongPass123!",
            "description": "Password (at least 6 characters)"
          },
          "confirmPassword": {
            "type": "string",
            "example": "StrongPass123!",
            "description": "Confirm password (at least 6 characters)"
          }
        },
        "required": [
          "email",
          "username",
          "password",
          "confirmPassword"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "alex.johnson@example.com"
          },
          "password": {
            "type": "string",
            "example": "StrongPass123!"
          },
          "role": {
            "type": "string",
            "example": "policyholder",
            "enum": [
              "policyholder",
              "insurance_admin"
            ]
          },
          "status": {
            "type": "string",
            "example": "active"
          },
          "kyc_status": {
            "type": "string",
            "example": "verified"
          },
          "location": {
            "type": "string",
            "example": "New York, USA"
          },
          "login_attempts": {
            "type": "number",
            "example": 0
          },
          "notes": {
            "type": "string",
            "example": "Test user"
          },
          "company": {
            "type": "string",
            "example": "Farm Corp Ltd"
          },
          "policies": {
            "type": "number",
            "example": 3
          },
          "claims": {
            "type": "number",
            "example": 2
          }
        },
        "required": [
          "email",
          "password",
          "role"
        ]
      }
    }
  }
}