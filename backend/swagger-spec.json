  "info": {
    "title": "My API",
    "description": "The backend API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "supabase-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "description": "Enter your Supabase access token"
      }
    },
    "schemas": {
      "CommonResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "email_verified": {
            "type": "boolean"
          },
          "username": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "lastSignInAt": {
            "type": "string"
          },
          "provider": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "email",
          "email_verified",
          "username",
          "role",
          "lastSignInAt",
          "provider"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserResponseDto"
          }
        },
        "required": [
          "accessToken",
          "user"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "johndoe@example.com",
            "description": "User email address"
          },
          "password": {
            "type": "string",
            "example": "secret123",
            "description": "Password for the user"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "yizunwong@gmail.com",
            "description": "User email address"
          },
          "username": {
            "type": "string",
            "example": "johndoe",
            "description": "Unique username for the user"
          },
          "password": {
            "type": "string",
            "example": "StrongPass123!",
            "description": "Password (at least 6 characters)"
          },
          "confirmPassword": {
            "type": "string",
            "example": "StrongPass123!",
            "description": "Confirm password (at least 6 characters)"
          }
        },
        "required": [
          "email",
          "username",
          "password",
          "confirmPassword"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "alex.johnson@example.com"
          },
          "password": {
            "type": "string",
            "example": "StrongPass123!"
          },
          "role": {
            "type": "string",
            "example": "policyholder",
            "enum": [
              "policyholder",
              "insurance_admin"
            ]
          },
          "status": {
            "type": "string",
            "example": "active"
          },
          "kyc_status": {
            "type": "string",
            "example": "verified"
          },
          "location": {
            "type": "string",
            "example": "New York, USA"
          },
          "login_attempts": {
            "type": "number",
            "example": 0
          },
          "notes": {
            "type": "string",
            "example": "Test user"
          },
          "company": {
            "type": "string",
            "example": "Farm Corp Ltd"
          },
          "policies": {
            "type": "number",
            "example": 3
          },
          "claims": {
            "type": "number",
            "example": 2
          }
        },
        "required": [
          "email",
          "password",
          "role"
        ]
      },
      "CreateClaimDto": {
        "type": "object",
        "properties": {
          "policy_id": {
            "type": "number",
            "example": 1,
            "description": "ID of the policy associated with the claim"
          },
          "claim_type": {
            "type": "string",
            "example": "accident",
            "description": "Type of the claim"
          },
          "amount": {
            "type": "number",
            "example": 1000,
            "description": "Amount claimed"
          },
          "description": {
            "type": "string",
            "example": "Accident on highway, car damaged.",
            "description": "Description of the claim"
          },
          "documents": {
            "description": "Documents to upload",
            "type": "array",
            "items": {
              "type": "string",
              "format": "binary"
            }
          }
        },
        "required": [
          "policy_id",
          "claim_type",
          "amount"
        ]
      },
      "UpdateClaimDto": {
        "type": "object",
        "properties": {
          "policy_id": {
            "type": "number",
            "example": 1,
            "description": "ID of the policy associated with the claim"
          },
          "claim_type": {
            "type": "string",
            "example": "accident",
            "description": "Type of the claim"
          },
          "amount": {
            "type": "number",
            "example": 1000,
            "description": "Amount claimed"
          },
          "description": {
            "type": "string",
            "example": "Accident on highway, car damaged.",
            "description": "Description of the claim"
          },
          "documents": {
            "description": "Documents to upload",
            "type": "array",
            "items": {
              "type": "string",
              "format": "binary"
            }
          },
          "id": {
            "type": "number",
            "example": 1,
            "description": "ID of the claim to update"
          },
          "status": {
            "type": "string",
            "example": "pending",
            "description": "Status of the claim"
          }
        },
        "required": [
          "id",
          "status"
        ]
      },
      "CreateDocumentsDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Policy Terms",
            "description": "Document name"
          },
          "url": {
            "type": "string",
            "example": "https://example.com/policy-terms.pdf",
            "description": "Document URL"
          }
        },
        "required": [
          "name",
          "url"
        ]
      },
      "CreateReviewsDto": {
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string",
            "example": "4f2a3343-1234-4b2d-9e13-8bbf0e2f748b",
            "description": "User ID (UUID)"
          },
          "rating": {
            "type": "number",
            "example": 5,
            "description": "Rating between 1 and 5"
          },
          "comment": {
            "type": "string",
            "example": "Excellent coverage and service.",
            "description": "Optional user comment"
          }
        },
        "required": [
          "user_id",
          "rating"
        ]
      },
      "CreatePolicyDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Comprehensive Health Coverage"
          },
          "category": {
            "type": "string",
            "example": "health"
          },
          "provider": {
            "type": "string",
            "example": "HealthSecure"
          },
          "coverage": {
            "type": "number",
            "example": 100000
          },
          "premium": {
            "type": "string",
            "example": "0.8 ETH/month"
          },
          "rating": {
            "type": "number",
            "example": 4.8
          },
          "popular": {
            "type": "boolean",
            "example": false
          },
          "features": {
            "example": [
              "Emergency Care",
              "Prescription Drugs",
              "Mental Health"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "description": {
            "type": "string",
            "example": "Complete healthcare coverage with blockchain-verified claims processing"
          },
          "documents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateDocumentsDto"
            }
          },
          "reviews": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateReviewsDto"
            }
          }
        },
        "required": [
          "name",
          "category",
          "provider",
          "coverage",
          "premium",
          "rating",
          "popular",
          "features",
          "documents"
        ]
      },
      "UpdatePolicyDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Comprehensive Health Coverage"
          },
          "category": {
            "type": "string",
            "example": "health"
          },
          "provider": {
            "type": "string",
            "example": "HealthSecure"
          },
          "coverage": {
            "type": "number",
            "example": 100000
          },
          "premium": {
            "type": "string",
            "example": "0.8 ETH/month"
          },
          "rating": {
            "type": "number",
            "example": 4.8
          },
          "popular": {
            "type": "boolean",
            "example": false
          },
          "features": {
            "example": [
              "Emergency Care",
              "Prescription Drugs",
              "Mental Health"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "description": {
            "type": "string",
            "example": "Complete healthcare coverage with blockchain-verified claims processing"
          },
          "documents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateDocumentsDto"
            }
          },
          "reviews": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateReviewsDto"
            }
          }
        }
      }
    }
  }
